package com.hospital.vo;

import java.sql.Date;

//病历表
public class MedicalRecord {
	private int meid;//病历编号
	private int patid;//患者编号
	private int mepatage;//患者年龄
	private String patsymptom;//患者病症
	private String docdiagnosis;//医生诊断
	private String druglist;//药品清单
	private Date medate;//就诊时间
	private String medocname;//医生姓名
	public int getMeid() {
		return meid;
	}
	public void setMeid(int meid) {
		this.meid = meid;
	}
	public int getPatid() {
		return patid;
	}
	public void setPatid(int patid) {
		this.patid = patid;
	}
	public int getMepatage() {
		return mepatage;
	}
	public void setMepatage(int mepatage) {
		this.mepatage = mepatage;
	}
	public String getPatsymptom() {
		return patsymptom;
	}
	public void setPatsymptom(String patsymptom) {
		this.patsymptom = patsymptom;
	}
	public String getDocdiagnosis() {
		return docdiagnosis;
	}
	public void setDocdiagnosis(String docdiagnosis) {
		this.docdiagnosis = docdiagnosis;
	}
	public String getDruglist() {
		return druglist;
	}
	public void setDruglist(String druglist) {
		this.druglist = druglist;
	}
	public Date getMedate() {
		return medate;
	}
	public void setMedate(Date medate) {
		this.medate = medate;
	}
	public String getMedocname() {
		return medocname;
	}
	public void setMedocname(String medocname) {
		this.medocname = medocname;
	}
	public MedicalRecord(int meid, int patid, int mepatage, String patsymptom, String docdiagnosis, String druglist,
			Date medate, String medocname) {
		super();
		this.meid = meid;
		this.patid = patid;
		this.mepatage = mepatage;
		this.patsymptom = patsymptom;
		this.docdiagnosis = docdiagnosis;
		this.druglist = druglist;
		this.medate = medate;
		this.medocname = medocname;
	}
	public MedicalRecord() {
		super();
	}
	
}
